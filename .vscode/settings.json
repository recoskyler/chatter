{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "*-lock.json": false,
        "*.lock": false,
        "**/.classpath": false,
        "**/.factorypath": false,
        "**/.project": false,
        "**/.settings": false,
        "**/.vscode": false,
        "**/node_modules": false,
        ".svelte-kit": true,
        "node_modules": true,
        ".vscode": true
    },
    "explorerExclude.backup": {},
    "cSpell.words": [
        "regen",
        "writables"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "eslint.validate": ["javascript", "typescript"],
    "svelte.plugin.svelte.defaultScriptLanguage": "ts",
    "svelte.trace.server": "messages",
    "editor.formatOnSave": true,
    "typescript.tsdk": "node_modules/typescript/lib"
}
