VITE_API_KEY=
VITE_MODEL=gpt-3.5-turbo

# Optional, 3000 will be used by default
APP_PORT=3000

# Optional, "chatter" will be used by default
DB_USER=chatter

DB_PASSWORD=
DB_ROOT_PASSWORD=

# Optional, "chatter" will be used by default
DB_DATABASE=chatter

# Optional, 3306 will be used by default
DB_FORWARD_PORT=

# This was inserted by `prisma init`:
